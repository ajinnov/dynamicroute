# Database Configuration
POSTGRES_DB=dynamicroute53
POSTGRES_USER=user
POSTGRES_PASSWORD=your-secure-password-here

# Backend Configuration
SECRET_KEY=your-very-long-and-secure-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=30
DATABASE_URL=postgresql://user:your-secure-password-here@db:5432/dynamicroute53

# CORS Configuration (comma-separated origins for production)
CORS_ORIGINS=["http://localhost:3000"]

# Frontend Configuration (React with Vite)
VITE_API_URL=http://localhost:8000

# Production Settings (uncomment and configure for production)
# VITE_API_URL=https://api.yourdomain.com
